

server {
    listen 8080;

     location /searchengine {

        proxy_pass http://host.docker.internal:5577/searchengine;
        proxy_redirect http://host.docker.internal:5577/searchengine $scheme://$server_name;
        proxy_set_header Host $host/searchengine;
    }

    location /send_file/ {
    internal;
    alias /{{ apps_folder }}/data_dump/;   # real file storage
}
}
